pipeline {
  
  agent any
  
    stages {
	    
		stage('init') {
	        steps {
	            echo 'initializing'
		 }
        }	
        stage('Build') {
            steps {
		        echo 'building'
		  }
        }
		stage('deployment') {
		    steps {
		        echo 'deploying the project'
		  }
		}
		
	
	}

}